digraph "01-simple-boolean" {
  splines = polyline;
  rankdir = LR;
  node [
    shape = circle
    fontname = "Roboto"
    fontsize = 11
    width = 1.35
    margin = 0
  ];
  edge [
    fontname = "Roboto"
    fontsize = 11
  ];

  off [
    label = "encrypted = false,\nbutton raised"
  ];
  on [
    label = "encrypted = true,\nbutton pressed"
  ];

  off -> on [
    label = "click"
  ];
  on -> off [
    label = "click"
  ];
}
